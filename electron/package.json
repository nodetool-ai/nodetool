{
  "name": "nodetool-electron",
  "version": "0.6.2-rc.26",
  "description": "node-based AI development tool",
  "homepage": "https://nodetool.ai",
  "main": "dist-electron/main.js",
  "scripts": {
    "prebuild": "node scripts/download-micromamba.cjs",
    "download-micromamba": "node scripts/download-micromamba.cjs",
    "start": "electron .",
    "chat": "electron . --chat",
    "build": "tsc && vite build && electron-builder",
    "vite:build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "tsc && eslint . && jest",
    "dev": "vite",
    "preview": "vite preview",
    "predist": "npm run download-micromamba && npm run build",
    "dist": "npm run download-micromamba && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "author": "matti.georgi@gmail.com",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.57.0",
    "@types/gunzip-maybe": "^1.4.2",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.27",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.6.0",
    "electron": "35.7.5",
    "electron-builder": "^26.4.1",
    "electron-mock": "^0.5.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unbzip2-stream": "^1.4.3",
    "vite": "^6.4.1",
    "vite-plugin-electron": "^0.29.0"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.11"
  },
  "dependencies": {
    "@eslint/compat": "^1.2.6",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "bufferutil": "^4.0.9",
    "electron-log": "^5.2.0",
    "electron-updater": "^6.1.8",
    "fuse": "^0.12.1",
    "gunzip-maybe": "^1.4.2",
    "js-yaml": "^4.1.1",
    "node-stream-zip": "^1.15.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tar": "^7.4.3",
    "ws": "^8.18.0",
    "zustand": "^5.0.3"
  },
  "overrides": {
    "qs": ">=6.14.1",
    "express": ">=4.21.2",
    "body-parser": ">=1.20.3",
    "glob": ">=10.5.0"
  },
  "permissions": [
    "audioCapture"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodetool-ai/nodetool.git"
  }
}
